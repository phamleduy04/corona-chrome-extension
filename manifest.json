{
    "manifest_version": 2,
    "name": "Corona stats",
    "description": "Corona infomation, stats in 1 click",
    "version": "1.3",
    "author": "DuyCyberSec",
    "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'",
    "browser_action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "activeTab"
    ]
}